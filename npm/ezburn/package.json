{
  "name": "ezburn",
  "version": "0.1.0",
  "description": "An extremely fast JavaScript and CSS bundler and minifier.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khulnasoft/ezburn.git"
  },
  "scripts": {
    "postinstall": "node install.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "ezburn": "bin/ezburn"
  },
  "devDependencies": {
    "typescript": "4.9.5",
    "@types/node": "18.15.11"
  },
  "optionalDependencies": {
    "@ezburn/aix-ppc64": "0.1.0",
    "@ezburn/android-arm": "0.1.0",
    "@ezburn/android-arm64": "0.1.0",
    "@ezburn/android-x64": "0.1.0",
    "@ezburn/darwin-arm64": "0.1.0",
    "@ezburn/darwin-x64": "0.1.0",
    "@ezburn/freebsd-arm64": "0.1.0",
    "@ezburn/freebsd-x64": "0.1.0",
    "@ezburn/linux-arm": "0.1.0",
    "@ezburn/linux-arm64": "0.1.0",
    "@ezburn/linux-ia32": "0.1.0",
    "@ezburn/linux-loong64": "0.1.0",
    "@ezburn/linux-mips64el": "0.1.0",
    "@ezburn/linux-ppc64": "0.1.0",
    "@ezburn/linux-riscv64": "0.1.0",
    "@ezburn/linux-s390x": "0.1.0",
    "@ezburn/linux-x64": "0.1.0",
    "@ezburn/netbsd-arm64": "0.1.0",
    "@ezburn/netbsd-x64": "0.1.0",
    "@ezburn/openbsd-arm64": "0.1.0",
    "@ezburn/openbsd-x64": "0.1.0",
    "@ezburn/sunos-x64": "0.1.0",
    "@ezburn/win32-arm64": "0.1.0",
    "@ezburn/win32-ia32": "0.1.0",
    "@ezburn/win32-x64": "0.1.0"
